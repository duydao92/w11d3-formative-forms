extends layout
block content
    h2 Existing Users
    table
        thead
            th id
            th First Name
            th Last Name
            th Email
            th Age
            th Favorite Beatle
            th Likes Ice Cream
        tr
            each user in users
                td= user.id
                td #{user.firstName}
                td #{user.lastName}
                td #{user.email}
    
